{
  "$schema": "https://vega.github.io/schema/vega/v5.json",
  "background": "white",
  "padding": 5,
  "width": 250,
  "height": 300,
  "title": {
    "text": [
      "Histogram Showing Distribution of ",
      "Total Spent on House Districts in 2020"
    ],
    "subtitle": "Source - OpenSecrets",
    "frame": "group"
  },
  "style": "cell",
  "data": [
    {
      "name": "source_0",
      "url": "https://raw.githubusercontent.com/Jacob-McLaughlin/Jacob-McLaughlin.github.io/main/project/data/house_2020/district_totals_2020.csv",
      "format": {"type": "csv", "delimiter": ","},
      "transform": [
        {
          "type": "extent",
          "field": "Total_Spent_Mill",
          "signal": "bin_binned_false_step_4_Total_Spent_Mill_extent"
        },
        {
          "type": "bin",
          "field": "Total_Spent_Mill",
          "as": [
            "bin_binned_false_step_4_Total_Spent_Mill",
            "bin_binned_false_step_4_Total_Spent_Mill_end"
          ],
          "signal": "bin_binned_false_step_4_Total_Spent_Mill_bins",
          "extent": {
            "signal": "bin_binned_false_step_4_Total_Spent_Mill_extent"
          },
          "binned": false,
          "step": 4
        },
        {
          "type": "aggregate",
          "groupby": [
            "bin_binned_false_step_4_Total_Spent_Mill",
            "bin_binned_false_step_4_Total_Spent_Mill_end"
          ],
          "ops": ["count"],
          "fields": [null],
          "as": ["__count"]
        },
        {
          "type": "filter",
          "expr": "isValid(datum[\"bin_binned_false_step_4_Total_Spent_Mill\"]) && isFinite(+datum[\"bin_binned_false_step_4_Total_Spent_Mill\"])"
        }
      ]
    }
  ],
  "marks": [
    {
      "name": "marks",
      "type": "rect",
      "style": ["bar"],
      "from": {"data": "source_0"},
      "encode": {
        "update": {
          "fill": {"value": "darkgoldenrod"},
          "ariaRoleDescription": {"value": "bar"},
          "description": {
            "signal": "\"Amount Spent on District in Million $ (Binned): \" + (!isValid(datum[\"bin_binned_false_step_4_Total_Spent_Mill\"]) || !isFinite(+datum[\"bin_binned_false_step_4_Total_Spent_Mill\"]) ? \"null\" : format(datum[\"bin_binned_false_step_4_Total_Spent_Mill\"], \"\") + \" â€“ \" + format(datum[\"bin_binned_false_step_4_Total_Spent_Mill_end\"], \"\")) + \"; Number of Districts in Range: \" + (format(datum[\"__count\"], \"\"))"
          },
          "x2": {
            "scale": "x",
            "field": "bin_binned_false_step_4_Total_Spent_Mill",
            "offset": 1
          },
          "x": {
            "scale": "x",
            "field": "bin_binned_false_step_4_Total_Spent_Mill_end"
          },
          "y": {"scale": "y", "field": "__count"},
          "y2": {"scale": "y", "value": 0}
        }
      }
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "linear",
      "domain": {
        "signal": "[bin_binned_false_step_4_Total_Spent_Mill_bins.start, bin_binned_false_step_4_Total_Spent_Mill_bins.stop]"
      },
      "range": [0, {"signal": "width"}],
      "bins": {"signal": "bin_binned_false_step_4_Total_Spent_Mill_bins"},
      "zero": false
    },
    {
      "name": "y",
      "type": "linear",
      "domain": {"data": "source_0", "field": "__count"},
      "range": [{"signal": "height"}, 0],
      "nice": true,
      "zero": true
    }
  ],
  "axes": [
    {
      "scale": "y",
      "orient": "left",
      "tickCount": 10,
      "gridScale": "x",
      "grid": true,
      "domain": false,
      "labels": false,
      "aria": false,
      "maxExtent": 0,
      "minExtent": 0,
      "ticks": false,
      "zindex": 0
    },
    {
      "scale": "x",
      "orient": "bottom",
      "grid": false,
      "title": "Amount Spent on District in Million $ (Binned)",
      "titleFontSize": 9,
      "titleFontWeight": "bold",
      "labelFlush": true,
      "labelOverlap": true,
      "tickCount": {"signal": "ceil(width/10)"},
      "zindex": 0
    },
    {
      "scale": "y",
      "orient": "left",
      "grid": false,
      "title": "Number of Districts in Range",
      "tickCount": 10,
      "titleFontSize": 9,
      "titleFontWeight": "bold",
      "labelOverlap": true,
      "zindex": 0
    }
  ],
  "config": {}
}
