{
  "$schema": "https://vega.github.io/schema/vega/v5.json",
  "background": "white",
  "padding": 5,
  "width": 450,
  "height": 400,
  "title": {
    "text": [
      "Percentage Votes for Joe Biden in 2020 versus",
      "Poverty Rate in Georgia's counties (2020)"
    ],
    "subtitle": "%. Source: Georgia Secretary of State and USDA",
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10,
    "anchor": "start",
    "color": "black"
  },
  "style": "cell",
  "data": [
    {
      "name": "source_0",
      "url": "https://raw.githubusercontent.com/Jacob-McLaughlin/Jacob-McLaughlin.github.io/main/portfolio/data/Georgia_Poverty_Election.csv",
      "format": {"type": "csv", "delimiter": ","}
    },
    {
      "name": "data_0",
      "source": "source_0",
      "transform": [
        {
          "type": "filter",
          "expr": "isValid(datum[\"PCTPOVALL_2020\"]) && isFinite(+datum[\"PCTPOVALL_2020\"]) && isValid(datum[\"Biden_Percent\"]) && isFinite(+datum[\"Biden_Percent\"])"
        }
      ]
    },
    {
      "name": "data_1",
      "source": "source_0",
      "transform": [
        {
          "type": "regression",
          "x": "PCTPOVALL_2020",
          "y": "Biden_Percent",
          "extent": [0, 50],
          "as": ["PCTPOVALL_2020", "Biden_Percent"]
        }
      ]
    }
  ],
  "marks": [
    {
      "name": "layer_0_marks",
      "type": "symbol",
      "style": ["circle"],
      "from": {"data": "data_0"},
      "encode": {
        "update": {
          "opacity": {"value": 0.7},
          "size": {"value": 50},
          "fill": {"value": "black"},
          "ariaRoleDescription": {"value": "circle"},
          "description": {
            "signal": "\"Poverty Rate (%): \" + (format(datum[\"PCTPOVALL_2020\"], \"\")) + \"; Votes for Biden (%): \" + (format(datum[\"Biden_Percent\"], \"\"))"
          },
          "x": {"scale": "x", "field": "PCTPOVALL_2020"},
          "y": {"scale": "y", "field": "Biden_Percent"},
          "shape": {"value": "circle"}
        }
      }
    },
    {
      "name": "layer_1_marks",
      "type": "line",
      "style": ["line"],
      "sort": {"field": "datum[\"PCTPOVALL_2020\"]"},
      "from": {"data": "data_1"},
      "encode": {
        "update": {
          "stroke": {"value": "red"},
          "description": {
            "signal": "\"PCTPOVALL_2020: \" + (format(datum[\"PCTPOVALL_2020\"], \"\")) + \"; Biden_Percent: \" + (format(datum[\"Biden_Percent\"], \"\"))"
          },
          "x": {"scale": "x", "field": "PCTPOVALL_2020"},
          "y": {"scale": "y", "field": "Biden_Percent"},
          "defined": {
            "signal": "isValid(datum[\"PCTPOVALL_2020\"]) && isFinite(+datum[\"PCTPOVALL_2020\"]) && isValid(datum[\"Biden_Percent\"]) && isFinite(+datum[\"Biden_Percent\"])"
          }
        }
      }
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "linear",
      "domain": [0, 50],
      "range": [0, {"signal": "width"}],
      "nice": true,
      "zero": true
    },
    {
      "name": "y",
      "type": "linear",
      "domain": {
        "fields": [
          {"data": "data_0", "field": "Biden_Percent"},
          {"data": "data_1", "field": "Biden_Percent"}
        ]
      },
      "range": [{"signal": "height"}, 0],
      "nice": true,
      "zero": true
    }
  ],
  "axes": [
    {
      "scale": "x",
      "orient": "bottom",
      "gridScale": "y",
      "grid": true,
      "tickCount": {"signal": "ceil(width/40)"},
      "domain": false,
      "labels": false,
      "aria": false,
      "maxExtent": 0,
      "minExtent": 0,
      "ticks": false,
      "zindex": 0
    },
    {
      "scale": "y",
      "orient": "left",
      "gridScale": "x",
      "grid": true,
      "tickCount": {"signal": "ceil(height/40)"},
      "domain": false,
      "labels": false,
      "aria": false,
      "maxExtent": 0,
      "minExtent": 0,
      "ticks": false,
      "zindex": 0
    },
    {
      "scale": "x",
      "orient": "bottom",
      "grid": false,
      "title": "Poverty Rate (%)",
      "labelFlush": true,
      "labelOverlap": true,
      "tickCount": {"signal": "ceil(width/40)"},
      "zindex": 0
    },
    {
      "scale": "y",
      "orient": "left",
      "grid": false,
      "title": "Votes for Biden (%)",
      "labelOverlap": true,
      "tickCount": {"signal": "ceil(height/40)"},
      "zindex": 0
    }
  ],
  "config": {}
}
